{
    "manifest_version": 3,
    "name": "ChatGPT history search",
    "version": "1.0.0",
    "description": "Browser extension for searching chat history",
    "host_permissions": ["https://chat.openai.com/*"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "img/logo-16.png",
        "48": "img/logo-19.png",
        "128": "img/logo-128.png"
      }
    }
  }